{
  "name": "crypto-orchestrator-mobile",
  "version": "1.0.0",
  "description": "CryptoOrchestrator Mobile App - iOS & Android",
  "main": "index.js",
  "scripts": {
    "init:native": "node -e \"const {execSync}=require('child_process');const isWin=process.platform==='win32';execSync(isWin?'powershell -ExecutionPolicy Bypass -File scripts/init-native.ps1':'bash scripts/init-native.sh',{stdio:'inherit'})\"",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "build:ios": "expo build:ios",
    "build:android": "expo build:android",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "type-check": "tsc --noEmit",
    "pods": "cd ios && pod install",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@tanstack/react-query": "^5.17.9",
    "axios": "^1.6.5",
    "babel-preset-expo": "^54.0.7",
    "dayjs": "^1.11.10",
    "expo": "^54.0.23",
    "expo-status-bar": "^3.0.8",
    "react": "^19.1.0",
    "react-native": "^0.81.5",
    "react-native-background-fetch": "^4.2.2",
    "react-native-biometrics": "^3.0.1",
    "react-native-chart-kit": "^6.12.0",
    "react-native-dotenv": "^3.4.9",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keychain": "^8.1.2",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-vector-icons": "^10.0.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/runtime": "^7.23.8",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.3",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.1.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
