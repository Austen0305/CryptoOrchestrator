{
  "name": "cryptoorchestrator",
  "version": "1.0.0",
  "main": "electron/index.js",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Austen0305/Crypto-Orchestrator.git"
  },
  "scripts": {
    "dev": "vite",
    "dev:web": "vite",
    "dev:fastapi": "python -m uvicorn server_fastapi.main:app --reload --host 0.0.0.0 --port 8000",
    "build": "vite build",
    "bundle:python": "node -e \"const {execSync}=require('child_process');const cmd=process.platform==='win32'?'powershell -ExecutionPolicy Bypass -File scripts/deployment/bundle_python_runtime.ps1':'bash scripts/deployment/bundle_python_runtime.sh';execSync(cmd,{stdio:'inherit'})\"",
    "build:electron": "npm run bundle:python && vite build && electron-builder",
    "electron": "cross-env NODE_ENV=development electron .",
    "electron:pack": "electron-builder --dir",
    "electron:dist": "electron-builder",
    "check": "tsc",
    "lint": "cross-env ESLINT_USE_FLAT_CONFIG=false node node_modules/eslint/bin/eslint.js client/src --ext .ts,.tsx --max-warnings 0",
    "format:check": "prettier --check \"client/src/**/*.{ts,tsx,json,css,scss,md}\"",
    "format": "prettier --write \"client/src/**/*.{ts,tsx,json,css,scss,md}\"",
    "cleanup": "powershell -ExecutionPolicy Bypass -File scripts/utilities/cleanup.ps1",
    "db:push": "drizzle-kit push",
    "test": "pytest server_fastapi/tests/ -v --cov=server_fastapi --cov-report=html",
    "test:watch": "pytest server_fastapi/tests/ -v --cov=server_fastapi -f",
    "test:frontend": "vitest --config client/vitest.config.ts",
    "test:frontend:ui": "vitest --config client/vitest.config.ts --ui",
    "test:frontend:coverage": "vitest --config client/vitest.config.ts --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:complete": "node scripts/testing/test-e2e-complete.js",
    "test:puppeteer": "node scripts/testing/run-puppeteer-tests.js",
    "setup:typescript": "node scripts/setup-typescript-tools.js",
    "start:all": "node scripts/utilities/start-all-services.js",
    "validate:env": "node scripts/utilities/validate-environment.js",
    "check:services": "node scripts/utilities/check-services.js",
    "verify:build": "node scripts/verification/verify-build.js",
    "test:infrastructure": "python scripts/testing/test_infrastructure.py",
    "test:security": "python scripts/testing/test_security.py",
    "test:security:comprehensive": "python scripts/testing/test_security.py",
    "test:phase1": "npm run test:infrastructure",
    "test:phase2": "npm run test:security",
    "test:phase10": "npm run load:test:comprehensive",
    "test:all": "npm run test && npm run test:frontend && npm run test:e2e",
    "test:pre-deploy": "npm run test:all && npm run test:infrastructure && npm run test:security && npm run load:test:comprehensive",
    "ci:frontend": "npm run check && npm run test:frontend -- --runInBand --reporter=dot",
    "lint:py": "python -m flake8 server_fastapi/ tests/",
    "format:py": "python -m black server_fastapi/ tests/",
    "health": "node -e \"fetch('http://localhost:8000/health').then(r=>r.json()).then(console.log).catch(console.error)\"",
    "release:prepare": "node -e \"const {execSync}=require('child_process');const cmd=process.platform==='win32'?'powershell -ExecutionPolicy Bypass -File scripts/deployment/prepare-release.ps1':'bash scripts/deployment/prepare-release.sh';execSync(cmd,{stdio:'inherit'})\"",
    "release:dry-run": "python scripts/deployment/release_automation.py",
    "health:advanced": "node -e \"fetch('http://localhost:8000/health').then(r=>r.json()).then(d=>console.log(JSON.stringify(d,null,2))).catch(console.error)\"",
    "redis:start": "powershell -ExecutionPolicy Bypass -File scripts/utilities/start_redis.ps1",
    "celery:worker": "powershell -ExecutionPolicy Bypass -File scripts/utilities/start_celery.ps1",
    "celery:beat": "celery -A server_fastapi.celery_app beat --loglevel=info",
    "migrate": "alembic upgrade head",
    "migrate:create": "alembic revision --autogenerate -m",
    "migrate:rollback": "alembic downgrade -1",
    "setup:dev": "python scripts/setup/dev_setup.py",
    "setup": "python scripts/setup/complete_setup.py",
    "setup:env": "python scripts/setup/create_env_file.py",
    "setup:db": "python scripts/setup/init_database.py",
    "setup:verify": "python scripts/verification/comprehensive_feature_verification.py",
    "setup:health": "python scripts/setup/health_check.py",
    "setup:deps": "python scripts/setup/verify_dependencies.py",
    "setup:test": "python scripts/testing/test_setup_complete.py",
    "setup:verify-features": "python scripts/testing/verify_all_features.py",
    "verify:startup": "python scripts/verification/startup_verification.py",
    "verify:features": "python scripts/verification/feature_verification.py",
    "health:check": "node scripts/utilities/health-check.js",
    "cache:clear": "node scripts/utilities/clear-cache.js",
    "analyze:performance": "python scripts/utilities/performance-analyzer.py",
    "check:dependencies": "python scripts/utilities/dependency-checker.py",
    "check:quality": "python scripts/utilities/code-quality-check.py",
    "check:all": "node scripts/utilities/comprehensive-check.js",
    "optimize:responses": "python scripts/utilities/optimize-responses.py",
    "analyze:routes": "python scripts/utilities/route-analyzer.py",
    "audit:security": "npm audit && safety check || true",
    "coverage:analyze": "python scripts/utilities/test-coverage.py",
    "db:health": "python scripts/utilities/database-health.py",
    "quick-start": "node -e \"const {execSync}=require('child_process');const cmd=process.platform==='win32'?'powershell -ExecutionPolicy Bypass -File scripts/setup/quick-start.ps1':'bash scripts/setup/quick-start.sh';execSync(cmd,{stdio:'inherit'})\"",
    "load:test": "python scripts/utilities/load_test.py",
    "load:test:comprehensive": "python scripts/utilities/load_test.py --comprehensive",
    "test:chaos": "python scripts/testing/test_chaos.py",
    "test:interactive": "python scripts/testing/test_interactive.py",
    "test:coverage": "python scripts/monitoring/coverage_reporter.py --run-tests --badge --trends",
    "test:coverage:badge": "python scripts/monitoring/coverage_reporter.py --badge",
    "test:coverage:trends": "python scripts/monitoring/coverage_reporter.py --trends",
    "setup:complete": "bash scripts/setup/setup-complete.sh",
    "setup:complete:win": "powershell -ExecutionPolicy Bypass -File scripts/setup/setup-complete.ps1",
    "generate:test-data": "python scripts/utilities/generate_test_data.py",
    "monitor:performance": "python scripts/monitoring/monitor_performance.py --compare --report",
    "monitor:performance:baseline": "python scripts/monitoring/monitor_performance.py --set-baseline",
    "monitor:health": "python scripts/monitoring/health_monitor.py",
    "monitor:health:60s": "python scripts/monitoring/health_monitor.py --duration 60",
    "logs:analyze": "python scripts/monitoring/log_aggregator.py",
    "logs:analyze:json": "python scripts/monitoring/log_aggregator.py --json log_analysis.json",
    "test:visual": "python scripts/monitoring/test_visual_regression.py --mode compare",
    "test:visual:baseline": "python scripts/monitoring/test_visual_regression.py --mode baseline",
    "test:visual:headed": "python scripts/monitoring/test_visual_regression.py --mode compare --headed",
    "generate:api-client": "python scripts/utilities/api_client_generator.py",
    "todo:count": "node scripts/update_todo_progress.js --count",
    "todo:update": "node scripts/update_todo_progress.js",
    "bundle:analyze": "node scripts/utilities/bundle-analyze.js",
    "check:deps": "node scripts/utilities/check-deps.js",
    "validate:jwt-auth": "python scripts/testing/validate_jwt_auth.py",
    "validate:env:py": "python scripts/testing/validate_env_vars.py",
    "validate:all": "npm run validate:env && npm run validate:jwt-auth",
    "postinstall": "node -e \"try{const {execSync}=require('child_process');const cmd=process.platform==='win32'?'python\\\\python.exe -m pip install -r requirements.txt':(process.env.PYTHON||'python3')+' -m pip install -r requirements.txt';execSync(cmd,{stdio:'inherit'})}catch(e){console.log('Skipping pip install:', e.message)}\"",
    "precommit": "lint-staged",
    "validate:db-migrations": "python scripts/testing/validate_db_migrations.py",
    "validate:backend": "python scripts/testing/validate_backend_api.py",
    "validate:security": "npm audit && python scripts/testing/validate_security.py"
  },
